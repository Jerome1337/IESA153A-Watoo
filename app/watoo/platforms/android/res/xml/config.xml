<?xml version='1.0' encoding='utf-8'?>
<widget id="com.app.watoo" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <preference name="loglevel" value="DEBUG" />
    <feature name="Camera">
        <param name="android-package" value="org.apache.cordova.camera.CameraLauncher" />
    </feature>
    <feature name="Contacts">
        <param name="android-package" value="org.apache.cordova.contacts.ContactManager" />
    </feature>
    <splash density="land-hdpi" src="www/img/Default-568h@2x~iphone.png" />
    <splash density="land-ldpi" src="www/img/Default-568h@2x~iphone.png" />
    <splash density="land-mdpi" src="www/img/Default-568h@2x~iphone.png" />
    <splash density="land-xhdpi" src="www/img/Default-568h@2x~iphone.png" />
    <icon density="ldpi" src="www/img/icon-60.png" />
    <icon density="mdpi" src="www/img/icon-60.png" />
    <icon density="hdpi" src="www/img/icon-60.png" />
    <icon density="xhdpi" src="www/img/icon-60.png" />
    <name>Watoo</name>
    <description>
        A sample Apache Cordova application that responds to the deviceready event.
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Apache Cordova Team
    </author>
    <content src="index.html" />
    <access origin="*" />
</widget>
